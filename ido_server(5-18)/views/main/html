<% data1.forEach(function(item, index) { %>
var dialog, form;

dialog = $( "#dialog-survey<%=index+1%>" ).dialog({
  	autoOpen: false,
  	height: 700,
  	width: 980,
  	modal: true,
  	buttons: {
    	//"다음": updateTitle,
